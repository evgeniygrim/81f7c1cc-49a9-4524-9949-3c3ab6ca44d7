{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:vue/base",
        "plugin:vue/vue3-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": {
            "ts": "@typescript-eslint/parser",
            "<template>": "espree"
        }
    },
    "parser": "vue-eslint-parser",
    "plugins": [
        "@typescript-eslint",
        "vue"
    ],
    "rules": {
    }
}
